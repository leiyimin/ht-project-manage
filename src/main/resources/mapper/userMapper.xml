<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="htmap.pjmanage.dao.UserDao">

    <select id="getOne" resultType="htmap.pjmanage.entity.User" parameterType="java.lang.Integer">
        SELECT * FROM t_user where id =#{id} and disabled=0 order by id
    </select>
    <select id="getAll" resultType="htmap.pjmanage.entity.User" parameterType="java.lang.Integer">
        SELECT * FROM t_user where disabled=0 order by id
    </select>
    <delete id="delete" parameterType="java.lang.Integer">
        DELETE FROM t_user where id=#{id}
    </delete>
</mapper>