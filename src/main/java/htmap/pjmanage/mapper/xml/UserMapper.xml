<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="htmap.pjmanage.mapper.UserMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="htmap.pjmanage.entity.User">
        <id column="id" property="id" />
        <result column="user_name" property="userName" />
        <result column="name" property="name" />
        <result column="pass_word" property="passWord" />
        <result column="gender" property="gender" />
        <result column="dept" property="dept" />
        <result column="email" property="email" />
        <result column="disabled" property="disabled" />
        <result column="create_time" property="createTime" />
        <result column="logic_flag" property="logicFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_name, name, pass_word, gender, dept, email, disabled, create_time, logic_flag
    </sql>

</mapper>
